{
  "name": "@vensst/js-toolkit",
  "description": "@vensst/js-toolkit 是 JavaScript 工具库，整合了前端一些常用的 js 函数",
  "version": "2.0.0-beta.1",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "@vensst/js-toolkit",
    "js-toolkit",
    "@vensst",
    "js",
    "javascript",
    "js工具库"
  ],
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "browser": "lib/index.umd.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    }
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "clean": "rimraf lib",
    "build:ts": "tsc --build --clean",
    "build": "npm run clean && npm run build:ts && rollup -c --bundleConfigAsCjs",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "author": "vensst",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vensst/js-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/vensst/js-toolkit/issues"
  },
  "homepage": "https://vensst.github.io/js-toolkit/",
  "dependencies": {
    "js-cookie": "3.0.1",
    "vuepress-plugin-md-enhance": "^2.0.0-rc.99"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.5.4",
    "@vuepress/bundler-vite": "^2.0.0-rc.23",
    "@vuepress/plugin-markdown-include": "^2.0.0-rc.120",
    "@vuepress/plugin-search": "^2.0.0-rc.105",
    "@vuepress/theme-default": "^2.0.0-rc.105",
    "babel-loader": "8.2.5",
    "css-loader": "6.7.1",
    "docdash": "^2.0.2",
    "documentation": "^14.0.3",
    "jsdoc": "^4.0.5",
    "mathjs": "^14.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^3.29.5",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass-embedded": "^1.89.0",
    "style-loader": "3.3.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.8.3",
    "vuepress": "^2.0.0-rc.23"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "ie >= 8",
    "chrome >= 49"
  ]
